{
    "arrowParens": "always",
    "bracketSpacing": true,
    "insertPragma": false,
    "printWidth": 100,
    "proseWrap": "always",
    "quoteProps": "as-needed",
    "semi": false,
    "singleQuote": false,
    "tabWidth": 4,
    "trailingComma": "all",
    "useTabs": false,
    "bracketSameLine": false,
    "endOfLine": "lf"
}
